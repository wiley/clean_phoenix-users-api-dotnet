{
	"name": "users-api container",
	"dockerComposeFile": [
        "../dev/docker-compose.yml"
    ],
	"service": "usersapi",
    "workspaceFolder": "/app",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-dotnettools.csharp",
				"josefpihrt-vscode.roslynator",
